# .env.example
#
# This is an example file for environment variables.
# For local development, create a copy of this file named ".env"
# and fill in your actual database connection details.
#
# DO NOT commit the .env file to version control.

# PostgreSQL Connection URL for local development
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://postgres:your_password@localhost:5432/your_database_name"

# You can leave these other variables as they are if you're only working on the database locally,
# but you would need to fill them out for the full application to run.
SESSION_SECRET="a_random_secret_string_for_sessions"
DISCORD_CLIENT_ID=""
DISCORD_CLIENT_SECRET=""
APP_BASE_URL="http://localhost:3000"
RAID_HELPER_API_KEY=""